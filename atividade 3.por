programa {
  funcao inicio() {
    inteiro disciplinas = 6
    inteiro tempo_minutos = 100
    inteiro tempo_disciplina 
    inteiro tempo_lazer
    tempo_disciplina = tempo_minutos / disciplinas
    tempo_lazer = tempo_minutos % disciplinas
    escreva("Você deve estudar ", tempo_disciplina, " minutos por disciplina, e você terá ", tempo_lazer," minutos de lazer")
  }
}
